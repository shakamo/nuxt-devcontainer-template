{
	"name": "Frontend",
	"dockerComposeFile": [
		"../docker-compose.yml"
	],
	"service": "frontend-app",
	"workspaceFolder": "/app",
	"remoteUser": "node",
	"forwardPorts": [],
	"mounts": [],
	"postCreateCommand": "/bin/sh .devcontainer/postCreateCommand.sh",
	"customizations": {
		"vscode": {
			"settings": {
				"files.encoding": "utf8",
				"editor.formatOnSave": true
			},
			"extensions": [
				"vue.volar",
				"mechatroner.rainbow-csv",
				"bierner.markdown-mermaid",
				"Vue.vscode-typescript-vue-plugin"
			]
		}
	}
}